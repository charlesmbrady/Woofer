{{!-- USER'S HOME PAGE --}}

{{> nav/nav-block}}

<div class="main container-fluid">
  <div class="row row-body">
    <div class="col-md-3" id="left">
      <div id="userBox">
        <div id="userBackgroundImage" style="background-image: url({{{userImg}}});"></div>
        <div id="edit">
          <div><a class="edit link linkleft" href="/editprofile">Edit Profile</a><a class="edit link linkright" href="/profile">View
              Profile</a></div>
          <div class="addDog"><a class="link addlink" href="/survey">+Add Dog</a></div>
        </div>

        <div id="userDogs">
          {{#each dogs}}
          {{> dogs/dog-block}}
          {{/each}}

        </div>
      </div>
      <div id="settingsBox">

      </div>

    </div>


    <div class="col-md-9" id="right">
      <div id="map">map</div>
      <div id="match-display">
        {{#each matches}}
        {{> matches/dogMatch-block}}
        {{/each}}
      </div>


    </div>



  </div>
</div>
{{> modal/interaction-block}}
<!--end main container-->