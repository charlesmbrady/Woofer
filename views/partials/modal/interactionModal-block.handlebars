<div class="modal" id="interaction-invite" tabindex="-1" role="dialog">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
          <div id="ownerPic"></div>
          <div id="dogPic"></div>
        <h5 class="modal-title">Let's Plan a Play Date!</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>

      <div class="modal-body">
        <form>
          <div class="form-group">
            <label for="comment">What</label>
            <input type="text" class="form-control" id="comment" placeholder="Play Date at Dog Park">
          </div>
            <div class="form-group">
            <label for="exampleFormControlSelect1">Example select</label>
                <select class="form-control" id="location">
                </select>
            </div>
          <div class="form-group">
            <label for="date">When</label>
            <input type="date" class="form-control" id="date">
          </div>
          <div class="form-group">
            <label for="time">What Time</label>
            <input type="time" class="form-control" id="time">
          </div>
        </form>
      </div>
    

      <div class="modal-footer">
        <button id="interaction-create" type="button" class="btn">Send Invite</button>
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>